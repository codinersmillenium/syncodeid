  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1,
  "networks": {
    "local":{
      "bind": "127.0.0.1:8080",
      "type": "ephemeral"
    }
  }
