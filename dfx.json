{
  "canisters": {
    "main": {
      "type": "motoko",
      "main": "src/backend/main.mo"
    },
    "data_contest": {
      "type": "motoko",
      "main": "src/backend/app/contest.mo"
    },
    "frontend": {
      "dependencies": [        
        "main",
        "data_contest"
      ],
      "source": [
        "src/frontend/assets/view"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1,
  "networks": {
    "local":{
      "bind": "127.0.0.1:8080",
      "type": "ephemeral"
    }
  }
}